import matplotlib.pyplot as plt 
import numpy as np

x=np.arange(1,11,1)
y1=(2*x)+1
y2=(2*x*x)+2
plt.plot(x,y1,label='y=2x+1',linewidth=3)
plt.plot(x,y2,label='y=2x^2+2',linewidth=3)
plt.legend()
plt.show()
